<Window x:Class="AWS_1.ImageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AWS_1"
        xmlns:uc="clr-namespace:AWS_1.User_Control"
        mc:Ignorable="d"
        Title="ImageWindow" Height="650" Width="1000" Background="#243665">
    <Grid>
        <Button x:Name="btnFindObject" Content="Find Object" Background="#243665" BorderThickness="3" BorderBrush="#8BD8BD"  FontWeight="Bold" Foreground="White" FontSize="14" HorizontalAlignment="Left" Margin="425,550,0,0" VerticalAlignment="Top" Width="150" Height="35" Click="FindObject"/>
        <ScrollViewer HorizontalAlignment="Left" Height="501" Margin="46,14,0,0" VerticalAlignment="Top" Width="900" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Visible">
            <uc:PictureBox x:Name="pbMyPictureBox" Width="876" Height="467"/>
        </ScrollViewer>
    </Grid>
</Window>
